*
	padding 0
	margin 0
a
	cursor pointer
ul
	list-style none
body
	background #fff
	font-size 12px
	font-family Tahoma,"宋体"
	width 100%
	& .top-tit
		margin 0 auto
		width 904px
		height 87px
		background-image url(https://aq.qq.com/v2/images/header.jpg)
		background-position -3px 0 
		& a:hover
			text-decoration underline
			color #2375b1
		& .tit-icon
			width 50%
			float left
			height 87px
		& .right-menu
			float right
			font-size 0
			padding 10px 0 0 0
			line-height 19px
			&>div
				display block
				font-size 12px
				padding-left 9px
				float left
			& .diff
				padding-left 3px
				height 17px
				position relative
				z-index 10
				top -1px
				& a.cl
					display inline-block
					position relative
					line-height 12px
					border 1px solid #fff
					padding-left 5px
					z-index 1
					background #fff
					padding-top 3px
					padding-bottom 1px
					& i.icon
						display inline-block
						width 8px
						height 8px
						background url(../img/12.PNG)
						background-position -903px -133px
						margin-left 6px
						margin-right 2px
				& .sele
					width 193px
					background #fff
					position absolute
					border 1px solid #959595
					top 17px
					padding 10px
					right 0
					display none
					&:before
						content ""
						width 1px
						height 10px
						position absolute
						top -2px
						right -1px
						background #959595
						z-index 3
					& dl
						& dt
							color #2375b1
							font-weight bold
							display block
							padding 10px 0 0 0
							border-top 1px dashed #d6d6d6	
							margin-top 7px
							&:first-child
								padding-top 0
								margin-top 0
								border-top 1px dashed transparent
						& dd
							line-height 21px
							color #535353	
							font-size 12px
							padding-top 3px
			& .diff:hover a.cl
				border-top 1px solid #959595
				border-left 1px solid #959595
				border-right 1px solid #959595
			& .diff:hover .sele
				display block
			& .diff:hover i.icon
				transform rotate(180deg)
				transition transform 0.3s
	& .menu
		width 100%
		background-image url(https://aq.qq.com/v2/images/header_bg.jpg)
		background-repeat repeat-x
		background-position 0 -86px
		height 41px
		& .inMenu
			margin 0 auto
			width 904px
			position relative
			ul
				& li
					float left
					line-height 41px
					font-size 18px
					color #fff
					margin-right 30px
					&>a:hover+.sele
						display block
					& .sele
						position absolute
						display none
						padding 0 2px 8px
						border-radius 4px
						box-shadow 0 0 0 1px #e0e0e0 inset
						top 41px
						background-color #fff
						background-image url(../img/bg.jpg)
						background-repeat repeat-x	
						z-index 3
						&:hover
							display block
						&>a
							display block
							color #266392
							line-height 42px
							padding 0 0px 0 40px
							font-size 14px
							font-family Microsoft YaHei
							position relative
							z-index 99
							&:hover
								background-color rgb(210, 234, 253)
							& .i-icon
								display block
								position absolute
								left 8px
								top 6px
								width 30px
								height 30px
								background-image url(../img/xiala.png)
								background-position -442px -89px
							&:hover .i-icon 
								background-position -597px -89px
								
					&>a
						display block
						padding 0 16px
						&:hover
							background-image url(../img/22.png)
							background-repeat repeat-x
					&>a.cur
						background-image url(../img/22.png)
						background-repeat repeat-x
				& .twoLi
					& .sele
						width 182px
				& .threeLi
					& .sele
						width 150px
				& .fourLi
					& .sele
						width 144px
			
			& .mobile-icon
				display block
				width 135px
				height 60px
				position absolute
				right 10px
				top -22px
				background-image url(../img/12.png)
				background-position -740px -1190px
				&:hover
					background-position -740px -1250px
	& .banner
		width 100%
		background-color #e9f1f4
		padding-top 10px
		& .codeFix
			padding 20px 15px 15px
			border 1px solid #bfd8e1
			box-shadow 0 1px 5px #bfd8e1
			position absolute 
			top 250px 
			right 50px
			z-index 1000
			width 112px
			background-color #fff
			p
				text-align center
				font-size 12px
			img
				margin-top 5px
				float left
			.icon-text
				width 102px
				height 17px
				display block
				margin auto
				background url(https://aq.qq.com/v2/images/aq_add_about.png) no-repeat center
			i.dele
				display block
				position absolute
				top 5px
				right 5px
				width 10px
				height 10px
				background url(../img/bg_close_qr.png) no-repeat center
				cursor pointer
		& .inner
			width 910px
			margin auto
			min-height 450px
			overflow hidden
			background url(https://aq.qq.com/v2/images/index_center.jpg) left top repeat-y
			& .center
				float left
				width 620px
				min-height 400px
				background-color yellow
				overflow hidden
				a:hover
					text-decoration underline
				& .bannerGai
					width 620px
					height 230px
					background red
				& .textIn
					padding-left 20px
					& .textIn-left
						width 300px
						float left
						& .smallTit
							width 200px
							height 18px
							margin-top 22px
							margin-bottom 8px
							background-image url(../img/tit.png)
						& .pic
							overflow hidden
							padding-top 5px
							padding-bottom 10px
							img
								float left
								padding 1px
								border 1px solid #e7e7e7
								margin-right 9px
							a 
								color #266392
								font-size 14px
							p
								padding-top 5px
								line-height 18px
								font-size 12px
					& .textIn-right
						width 245px
						height 300px
						padding-left 40px
						padding-right 15px
						float left
						background-color red
			& .rightMain
				float left
				min-height 400px
				background-color skyblue
				width 250px
				padding 15px 20px 0 15px
					
			
		& .add2
			clear both
			width 910px
			margin 0 auto
			height 5px
			background-image url(../img/12.png)
			background-position 0 -456px
	& .bottom
		width 915px	
		margin auto
		text-align center
		margin-top 10px
		padding 15px 0
		color #999
		font-size 12px
		line-height 21px
		& .top
			a
				margin 0 7px
				&:hover
					text-decoration underline
					color rgb(189, 10, 1)