*
	padding 0
	margin 0
a
	cursor pointer
ul
	list-style none
body
	background #fff
	font-size 12px
	font-family "宋体"
	width 100%
	& .top-tit
		margin 0 auto
		width 904px
		height 87px
		background-image url(https://aq.qq.com/v2/images/header.jpg)
		background-position -3px 0 
		& a:hover
			text-decoration underline
			color #2375b1
		& .tit-icon
			width 50%
			float left
			height 87px
		& .right-menu
			float right
			font-size 0
			padding 10px 0 0 0
			line-height 19px
			&>div
				display block
				font-size 12px
				padding-left 9px
				float left
				
			&>.diff
				width 64px
				position relative
				z-index 10
				font-size 0
				padding-right 4px
				& a.cl
					display block
					& span
						font-size 12px
						display block
						float left
					& i.icon
						display block
						float right
						margin-top 4px
						width 12px
						height 8px
						background url(../img/12.PNG)
						background-position -901px -133px	
				& .ad1
					display none
					width 70px
					height 19px
					position absolute
					top -3px
					right 0
					z-index 3
					border 1px solid #959595
					border-bottom 1px solid #fff
				& .sele
					font-size 12px
					width 193px
					background #fff
					position absolute
					border 1px solid #959595
					top 17px
					padding 10px
					right 0
					display none
					& .ad2
						width 1px
						height 10px
						position absolute
						top -2px
						right -1px
						background #959595
						z-index 3
					& dl
						& dt
							color #2375b1
							font-weight bold
							display block
							padding 10px 0 0 0
							border-top 1px dashed #d6d6d6	
							margin-top 7px
							&:first-child
								padding-top 0
								margin-top 0
								border-top 1px dashed transparent
						& dd
							line-height 21px
							color #535353	
							font-size 12px
							padding-top 3px
		
			& .diff:hover .sele
				display block
			& .diff:hover .ad1
				display block
			& .diff:hover i.icon
				transform rotate(180deg)
				transition transform 0.3s
	& .menu
		width 100%
		background-image url(https://aq.qq.com/v2/images/header_bg.jpg)
		background-repeat repeat-x
		background-position 0 -86px
		position relative 
		z-index 2
		height 41px
		& .inMenu
			margin 0 auto
			width 904px
			position relative
			ul
				& li
					float left
					line-height 41px
					font-size 18px
					color #fff
					margin-right 30px
					&>a
						color #fff
						text-decoration none
					&>a:hover+.sele
						display block
					& .sele
						position absolute
						display none
						padding 0 2px 8px
						border-radius 4px
						box-shadow 0 0 0 1px #e0e0e0 inset
						top 41px
						background-color #fff
						z-index 3
						overflow hidden
						padding-top 4px
						&:hover
							display block
						&>a
							display block
							color #266392
							height 42px
							line-height 42px
							padding 0 0px 0 40px
							font-size 14px
							font-family Microsoft YaHei
							position relative
							z-index 99
							&:hover
								background-color #d2eafd
							& .i-icon
								display block
								position absolute
								left 8px
								top 6px
								width 30px
								height 30px
								background-image url(../img/xiala.png)
								background-position -442px -89px
							&:hover .i-icon 
								background-position -597px -89px
								
					&>a
						display block
						padding 0 16px
						&:hover
							background-image url(../img/22.png)
							background-repeat repeat-x
					&>a.cur
						background-image url(../img/22.png)
						background-repeat repeat-x
				& .twoLi
					& .sele
						width 182px
				& .threeLi
					& .sele
						width 150px
				& .fourLi
					& .sele
						width 144px
			
			& .mobile-icon
				display block
				width 135px
				height 60px
				position absolute
				right 10px
				top -22px
				background-image url(../img/12.png)
				background-position -740px -1190px
				&:hover
					background-position -740px -1250px
	& .banner
		width 100%
		background-color #e9f1f4
		padding-top 10px
		position relative
		& .codeFix
			padding 20px 15px 15px
			border 1px solid #bfd8e1
			box-shadow 0 1px 5px #bfd8e1
			position absolute 
			top 250px 
			right 50px
			z-index 1000
			width 112px
			background-color #fff
			p
				text-align center
				font-size 12px
			img
				margin-top 5px
				float left
			.icon-text
				width 102px
				height 17px
				display block
				margin auto
				background url(https://aq.qq.com/v2/images/aq_add_about.png) no-repeat center
			i.dele
				display block
				position absolute
				top 5px
				right 5px
				width 9px
				height 9px
				background url(../img/bg_close_qr.png) 50% 50% no-repeat
				cursor pointer
		& .inner
			width 910px
			margin auto
			min-height 450px
			overflow hidden
			background url(https://aq.qq.com/v2/images/index_center.jpg) left top repeat-y
			& .center
				float left
				width 620px
				min-height 400px
				overflow hidden
				a:hover
					text-decoration underline
				& .bannerGai
					width 620px
					height 273px
					overflow hidden
					position relative
					& span
						display block
						position absolute
						background-color black
						top 107px
						z-index 5
						width 30px
						height 60px
						cursor pointer 
						opacity .3
						background-image url(../img/jiantou.png)
						&.perv
							left 0	
							background-position 3px 8px	
						&.next
							right 0
							background-position 6px -45px	
						&:hover
							opacity .7
					& .container
						height 273px
						position relative
						z-index 0
						left -620px
						& a
							display block
							width 620px
							height 273px
							float left
							& img
								width 620px
								height 273px
					& .buttons
						position absolute 
						bottom 10px
						width 100%	
						left 0
						display inline-block
						margin 0 auto
						height 20px
						font-size 0
						text-align center
						line-height 20px 
						z-index 1
						img.imgBg
							position absolute	
							top -4px
							height 19px
							z-index 0
						em
							position relative 
							z-index 1
							display inline-block
							width 10px
							height 10px
							background #fff
							opacity 0.5
							margin 0 3px
							border-radius 5px
							cursor pointer
							&.cur
								opacity 1
							
				& .textIn
					padding-left 20px
					& .textIn-left
						width 300px
						float left
						& .smallTit
							width 200px
							height 18px
							margin-top 22px
							margin-bottom 8px
							background-image url(../img/tit.png)
						& .pic
							overflow hidden
							padding-top 5px
							padding-bottom 10px
							height 72px
							img
								float left
								padding 1px
								border 1px solid #e7e7e7
								margin-right 9px
							a 
								color #266392
								font-size 14px
							p
								padding-top 5px
								line-height 18px
								font-size 12px
					& .textIn-right
						width 245px
						padding-left 40px
						padding-right 15px
						float left
						position relative
						& .pub
							position relative
							margin-top 22px
							& p.cla
								
								margin-bottom 8px
								width 100%
								text-align left
								font-size 14px
								color gray
								font-weight bold
							& .page
								position absolute
								top 0
								right 0
								& em
									font-size 11px
									line-height 14px
									font-style normal
									color #80a4b8
									float left
									display block
									width 14px
									height 14px
									border-radius 7px
									border 1px solid #e4ecf0
									text-align center
									margin-left 2px
									background-color #f7f9fa
									cursor pointer
									&.cur
										background-color #54b2ed
										color #fff
							& .MainInText
								& ul
									& li
										height 24px
										font-size 14px
										line-height 17px
										display none
										& a
											color #266392
											overflow hidden
											width 160px
											float left
											display block
											text-overflow ellipsis
											white-space nowrap
										& .date
											float right
											color #b7b7b7
						& .pub1
							height 193px
						& .pub2
							height 121px
							margin-top 27px
						& .pub3
							height 121px
									
			& .rightMain
				float left
				min-height 400px
				width 250px
				padding 15px 20px 0 15px
				&>.topCla
					padding-top 8px
					overflow hidden
					height 43px
					display block
					i
						display block
						width 32px
						height 32px
						float left
						margin-right 10px
						background url(../img/icon.png) 0 -1657px no-repeat
						&.two-i
							background-position 0 -1615px
						&.three-i
							background-position 0 -1573px
						&.four-i
							background-position 0 -1909px
					& .tit
						color #333
						font-size 14px
						line-height 15px
					& .des
						font-size 12px
						line-height 15px
						padding-top 3px
						color #959595
					&:hover
						background url(../img/12.png) 0 -190px no-repeat
				& h3
					padding-bottom 6px
					border-bottom 1px dotted #cdcdcd
					margin-bottom 6px
				& .view
					color #266392
					padding-top 20px
					margin-bottom 0
				& .notice
					list-style-type square
					margin-left 16px
					color #266392
					padding-top 5px
					li
						margin-top 10px
						a
							line-height 20px
							font-size 12px
							white-space normal
							&:hover
								text-decoration underline
					
					
					
			
		& .add2
			clear both
			width 910px
			margin 0 auto
			height 5px
			background-image url(../img/12.png)
			background-position 0 -456px
	& .bottom
		width 915px	
		margin auto
		text-align center
		margin-top 10px
		padding 15px 0
		color #999
		font-size 12px
		line-height 21px
		& .top
			a
				margin 0 7px
				&:hover
					text-decoration underline
					color rgb(189, 10, 1)